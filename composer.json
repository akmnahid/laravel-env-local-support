    "authors": [
        {
            "name": "Abu Kahab Mohammad Nahid Hossain",
            "email": "mail@akmnahid.com",
            "role": "Developer",
            "homepage": "https://www.akmnahid.com"
        }
    ],
    "type": "library",
    "keywords": [
        "akmnahid",
        "laravel-env-local-support"
    ],
    "homepage": "https://github.com/akmnahid/laravel-env-local-support",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Akmnahid\\LaravelEnvLocalSupport\\": "src/"
        }
